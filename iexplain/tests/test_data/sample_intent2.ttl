
@prefix icm:  <http://tio.models.tmforum.org/tio/v3.6.0/IntentCommonModel/> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix geo:  <http://www.opengis.net/ont/geosparql#> .
@prefix intendproject: <http://intendproject.eu/> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .
@prefix set: <http://example.org/set#> .
@prefix quan: <http://example.org/quan#> .

intendproject:I1 a icm:Intent ;
    dct:description "Intent for real-time, lossless 4K video streaming from a rescue drone in Tromsdalen, Norway to an object detection classification model and GUI."@en ;
    log:allOf ( intendproject:DE1 intendproject:RE1 ) ;
.

intendproject:DE1 a icm:DeliveryExpectation ;
    icm:target intendproject:inSwitch ;
    dct:description "Ensure uninterrupted 4K video streaming from the rescue drone to the object detection model and graphical user interface during the Tromsdalen rescue mission in Norway."@en ;
    log:allOf ( intendproject:C1 intendproject:C2 intendproject:C3 intendproject:CX1 ) ;
.

intendproject:C1 a icm:Condition ;
    dct:description "The maximum network latency for real-time video transmission must be less than 50 ms."@en ;
    set:forAll ( _:X
        [ icm:valuesOfTargetProperty ( intendproject:Telenor5GLatency ) ]
        quan:smaller ( _:X [ rdf:value "50"^^xsd:decimal ;
                               quan:unit "ms"
                             ] )
    ) ;
.

intendproject:C2 a icm:Condition ;
    dct:description "The network must provide a minimum bandwidth of 25 mbit/s to support high quality 4K video streaming."@en ;
    set:forAll ( _:X
        [ icm:valuesOfTargetProperty ( intendproject:Telenor5GBandwidth ) ]
        quan:larger ( _:X [ rdf:value "25"^^xsd:decimal ;
                            quan:unit "mbit/s"
                          ] )
    ) ;
.

intendproject:C3 a icm:Condition ;
    dct:description "The processing latency at the edge (ObjectDetectionModel) should be less than 20 ms for real-time classification."@en ;
    set:forAll ( _:X
        [ icm:valuesOfTargetProperty ( intendproject:TelenorEdgeComputeLatency ) ]
        quan:smaller ( _:X [ rdf:value "20"^^xsd:decimal ;
                               quan:unit "ms"
                            ] )
    ) ;
.

intendproject:CX1 a icm:Context ;
    dct:description "This context applies to the rescue operation area in Tromsdalen, Norway."@en ;
    intendproject:appliesToRegion intendproject:K1RescueMissionRegion ;
.

intendproject:K1RescueMissionRegion a geo:Feature ;
    dct:description "Geographical region covering the Tromsdalen area in Norway where the rescue drone mission is conducted."@en ;
    geo:hasGeometry [
        a geo:Polygon ;
        geo:asWKT "POLYGON((69.66 18.95, 69.66 18.97, 69.64 18.97, 69.64 18.95))"^^geo:wktLiteral
    ] ;
.

intendproject:RE1 a icm:ReportingExpectation ;
    icm:target intendproject:inSwitch ;
    dct:description "Report continuously on the status of the real-time 4K video delivery and object detection metrics to ensure the rescue mission requirements are met."@en ;
.
        